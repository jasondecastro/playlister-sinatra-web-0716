<form method="POST" action="/songs/new">
  <input type="text" name="song[artist]" id="Artist Name">
  <input type="text" name="song[name]" id="Name">

  
  <% Artist.all.each do |artist| %>

    <input type="checkbox" name="song[artist_ids][]" value="<%= artist.id %>" id="<%=artist.name %>"><%=artist.name%></input>
    
  <% end %>

   <% Genre.all.each do |genre| %>
      <input type="checkbox" name="song[genre_ids][]" value="<%= genre.id %>" id="<%=genre.name %>"><%=genre.name%></input>
  <% end %>

  <input type="submit" name="submit" id="Create" value="submit">

</form>
